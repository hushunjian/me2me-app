  <select id="getRandomDropaRound" resultMap="BaseResultMap" parameterType="java.util.Map">
    SELECT m.* FROM topic t,
    (SELECT * FROM topic_droparound
    <if test="set != ''">
    where topicId not in (${set})
    </if>
    ) m
    WHERE t.id=m.topicId
    AND t.uid != #{uid}
